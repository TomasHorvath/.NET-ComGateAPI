@model THsoftware.ComGate.Core.Domain.Models.ApiResponse<bool>
@{
	ViewBag.Title = "RefundComplete";
}


<div class="container">
	<h2>Refundace platby</h2>

	@if (Model.Success)
	{
		<div class="alert alert-success">
			<strong>@Model.Code.ToString()</strong>
		</div>

		<div class="alert alert-success">
			<strong>@Model.Message</strong>
		</div>
	}
	else
	{
		<div class="alert alert-danger">
			<strong>@Model.Code.ToString()</strong>
		</div>

		<div class="alert alert-danger">
			<strong>@Model.Message</strong>
		</div>
	}

</div>


